var _types = require("./types");

var _config = require("../config");

const headers = {
  "Content-Type": "application/json",
  Accept: "application/json"
};

const fetchCategories = () => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/categories`, headers).then(res => res.json()).then(data => dispatch({
    type: _types.FETCH_CATEGORIES,
    payload: data
  })).catch(err => console.error(err));
};

exports.fetchCategories = fetchCategories;

const deleteCategory = categoryId => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/categories/` + categoryId, {
    headers,
    method: "DELETE"
  }).then(res => res.json()).then(data => dispatch({
    type: _types.DELETE_CATEGORY,
    payload: data
  }));
};

exports.deleteCategory = deleteCategory;

const createCategory = category => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/categories`, {
    headers,
    method: "POST",
    body: JSON.stringify(category)
  }).then(res => res.json()).then(data => dispatch({
    type: _types.CREATE_CATEGORY,
    payload: data
  }));
};

exports.createCategory = createCategory;
