exports.createUser = exports.deleteUser = exports.fetchUsers = void 0;

var _types = require("./types");

var _config = require("../config");

const headers = {
  "Content-Type": "application/json",
  Accept: "application/json"
};

const fetchUsers = () => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/users`, headers).then(res => res.json()).then(data => dispatch({
    type: _types.FETCH_USERS,
    payload: data
  })).catch(err => console.error(err));
};

exports.fetchUsers = fetchUsers;

const deleteUser = userId => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/users/` + userId, {
    headers,
    method: "DELETE"
  }).then(res => res.json()).then(data => dispatch({
    type: _types.DELETE_USER,
    payload: data
  }));
};

exports.deleteUser = deleteUser;

const createUser = user => dispatch => {
  fetch(`${_config.API_ADMIN_URL}/users`, {
    headers,
    method: "POST",
    body: JSON.stringify(user)
  }).then(res => res.json()).then(data => dispatch({
    type: _types.CREATE_USER,
    payload: data
  }));
};

exports.createUser = createUser;
